{
	"schemaVersion": 1,
	"id": "bluewire",
	"version": "${version}",
	"name": "Blue Redstone wire mod",
	"description": "This mod can turn your redstone dust to blue!",
	"authors": [
		"WalkerTian"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/Walkersifolia/Bluewire"
	},
	"license": "CC0-1.0",
	"icon": "assets/bluewire/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.example.ExampleMod"
		],
		"client": [
			"com.example.ExampleModClient"
		]
	},
	"mixins": [
		"bluewire.mixins.json",
		{
			"config": "bluewire.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}